<?xml version="1.0" encoding="UTF-8"?>
<security>
    <constraint>
        <url-pattern>.+.jsp</url-pattern>
        <role>admin</role>
    </constraint>
    <constraint>
        <url-pattern>/viewHomePage</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
    <constraint>
        <url-pattern>/viewTools</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
    <constraint>
        <url-pattern>/viewLoginForm</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
    <constraint>
        <url-pattern>/loginUser</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
    <constraint>
        <url-pattern>/logoutUser</url-pattern>
        <role>admin</role>
        <role>client</role>
    </constraint>
    <constraint>
        <url-pattern>/viewRegisterForm</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
    <constraint>
        <url-pattern>/registerUser</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
    <constraint>
        <url-pattern>/executeOrder</url-pattern>
        <role>client</role>
    </constraint>
    <constraint>
        <url-pattern>/viewOrder</url-pattern>
        <role>client</role>
    </constraint>
    <constraint>
        <url-pattern>/badRequest</url-pattern>
        <role>admin</role>
        <role>client</role>
        <role>anyone</role>
    </constraint>
</security>